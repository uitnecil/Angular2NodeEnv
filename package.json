  "scripts": {
    "serve": "concurrently --kill-others \"npm run watch:server\" \"npm run watch:client\"",
    "watch:server": "export NODE_ENV=development npm run eslint && nodemon --watch server",
    "watch:client": "webpack-dev-server --config ./webpack/webpack.dev.js --inline",
    "build": "npm run build:client && npm run build:server",
    "build:server": "npm run copy:server && npm run copy:package",
    "build:client": "npm run clean:dist && npm run copy:videos && NODE_ENV=production webpack -p --config ./webpack/webpack.prod.js",
    "copy:videos": "n-copy --source 'client/assets/videos' --destination 'dist/client/assets/videos/' '*'",
    "clean:dist": "n-clean dist",
    "copy:server": "n-copy --source server --destination 'dist/server' '**/!(*.spec|*.integration).js'",
    "copy:package": "n-copy --destination dist 'package.json'",
    "test": "karma start ./karma/karma.conf.js",
    "eslint": "eslint server/.eslintrc"
  },
